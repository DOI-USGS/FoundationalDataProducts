	Issn = {0036-8075},
	Journal = {Science},
	Number = {5419},
	Pages = {1495--1503},
	Publisher = {American Association for the Advancement of Science},
	Title = {The Global Topography of {M}ars and Implications for Surface Evolution},
	Url = {https://science.sciencemag.org/content/284/5419/1495},
	Volume = {284},
	Year = {1999},
