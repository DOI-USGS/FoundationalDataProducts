	Issn = {1572-9672},
	Journal = {Space Science Reviews},
	Month = {Oct},
	Number = {1},
	Pages = {109--133},
	Title = {Analysis of Local Slopes at the {InSight} Landing Site on {M}ars},
	Url = {https://doi.org/10.1007/s11214-016-0292-x},
	Volume = {211},
