	Doi = {10.1126/science.1250551},
	Eprint = {https://science.sciencemag.org/content/344/6179/78.full.pdf},
	Issn = {0036-8075},
	Journal = {Science},
	Number = {6179},
	Pages = {78--80},
	Publisher = {American Association for the Advancement of Science},
	Title = {The Gravity Field and Interior Structure of Enceladus},
	Url = {https://science.sciencemag.org/content/344/6179/78},
	Volume = {344},
