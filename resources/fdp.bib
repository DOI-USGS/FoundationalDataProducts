	Keywords = {(Mars, interior), (Mars, polar caps), (Tides, solid body)},
	Pages = {228 - 245},
	Title = {Seasonal and static gravity field of {M}ars from {MGS}, {M}ars {O}dyssey and {MRO} radio science},
	Url = {http://www.sciencedirect.com/science/article/pii/S0019103516001287},
	Volume = {272},
	Year = {2016},
	Bdsk-Url-1 = {http://www.sciencedirect.com/science/article/pii/S0019103516001287},
